<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="medicalstore.project.com.onlinemedicalstore"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="medicalstore.project.com.onlinemedicalstore.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="medicalstore.project.com.onlinemedicalstore.QuantityActivity"
            android:label="@string/title_activity_quantity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="medicalstore.project.com.onlinemedicalstore.OrdersActivity"
            android:label="Orders"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="medicalstore.project.com.onlinemedicalstore.MedicineActivity"
            android:label="Available Medicines" >
        </activity>
    </application>

</manifest>